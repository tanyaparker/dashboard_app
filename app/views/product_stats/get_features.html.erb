<h1>Product Team Stats</h1>
 
<div class="container_12">
  <ul class="feature">
    <% for i in (0...@count) %>
      <% if @final_project["#{i}".to_i].story_type == "bug" %>
        <% if @final_project["#{i}".to_i].current_state == "accepted" %> 
              <li class="feat_accepted">
              <%= @final_project["#{i}".to_i].name %><span class="bugs"></span>
              </li>
            <% elsif @final_project["#{i}".to_i].current_state == "delivered" %>
              <li class="feat_delivered">
              <%= @final_project["#{i}".to_i].name %><span class="bugs"></span>
              </li> 
            <% elsif @final_project["#{i}".to_i].current_state == "finished" %>
              <li class="feat_finished">
              <%= @final_project["#{i}".to_i].name %><span class="bugs"></span>
              </li>
            <% elsif @final_project["#{i}".to_i].current_state == "rejected" %>
              <li class="feat_rejected">
              <%= @final_project["#{i}".to_i].name %><span class="bugs"></span>
              </li>     
            <% else %>
              <li class="feat_started">
              <%= @final_project["#{i}".to_i].name %><span class="bugs"></span>
              </li> 
        <% end %>    
      <% elsif @final_project["#{i}".to_i].story_type == "chore" %>  
        <% if @final_project["#{i}".to_i].current_state == "accepted" %> 
              <li class="feat_accepted">
              <%= @final_project["#{i}".to_i].name %><span class="chores"></span>
              </li>
            <% elsif @final_project["#{i}".to_i].current_state == "delivered" %>
              <li class="feat_delivered">
              <%= @final_project["#{i}".to_i].name %><span class="chores"></span>
              </li> 
            <% elsif @final_project["#{i}".to_i].current_state == "finished" %>
              <li class="feat_finished">
              <%= @final_project["#{i}".to_i].name %><span class="chores"></span>
              </li>
            <% elsif @final_project["#{i}".to_i].current_state == "rejected" %>
              <li class="feat_rejected">
              <%= @final_project["#{i}".to_i].name %><span class="chores"></span>
              </li>     
            <% else %>
              <li class="feat_started">
              <%= @final_project["#{i}".to_i].name %><span class="chores"></span>
              </li> 
        <% end %>
      <% elsif @final_project["#{i}".to_i].story_type == "feature" %>  
        <% if @final_project["#{i}".to_i].current_state == "accepted" %> 
              <li class="feat_accepted">
              <%= @final_project["#{i}".to_i].name %><span class="features"></span>
              </li>
            <% elsif @final_project["#{i}".to_i].current_state == "delivered" %>
              <li class="feat_delivered">
              <%= @final_project["#{i}".to_i].name %><span class="features"></span>
              </li> 
            <% elsif @final_project["#{i}".to_i].current_state == "finished" %>
              <li class="feat_finished">
              <%= @final_project["#{i}".to_i].name %><span class="features"></span>
              </li>
            <% elsif @final_project["#{i}".to_i].current_state == "rejected" %>
              <li class="feat_rejected">
              <%= @final_project["#{i}".to_i].name %><span class="features"></span>
              </li>     
            <% else %>
              <li class="feat_started">
              <%= @final_project["#{i}".to_i].name %><span class="features"></span>
              </li> 
        <% end %>
      <% else %>  
        <% if @final_project["#{i}".to_i].current_state == "accepted" %> 
              <li class="feat_accepted">
              <%= @final_project["#{i}".to_i].name %><span class="releases"></span>
              </li>
            <% elsif @final_project["#{i}".to_i].current_state == "delivered" %>
              <li class="feat_delivered">
              <%= @final_project["#{i}".to_i].name %><span class="releases"></span>
              </li> 
            <% elsif @final_project["#{i}".to_i].current_state == "finished" %>
              <li class="feat_finished">
              <%= @final_project["#{i}".to_i].name %><span class="releases"></span>
              </li>
            <% elsif @final_project["#{i}".to_i].current_state == "rejected" %>
              <li class="feat_rejected">
              <%= @final_project["#{i}".to_i].name %><span class="releases"></span>
              </li>     
            <% else %>
              <li class="feat_started">
              <%= @final_project["#{i}".to_i].name %><span class="releases"></span>
              </li> 
        <% end %>
      <% end %>  
    <% end %>
  </ul>
</div>